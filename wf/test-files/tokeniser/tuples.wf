---
Stuff to do with tuples
---

let positional = [1, 2, 3, 4, 5]
let positional_multiline = [
	1, 2, 3
	4, 5, 6,
	7, 8, 9
]
let named = [.a 1, .b 2, .c 3, .d 4, .e 5]
let named_multiline = [
	.a   1
	.`b` 2
	.c   3
]
let mix = [1, 2, .a 3, .b 4, 5]

let access_unblocked = mix.0 + mix.`1` + mix.a + mix.`b`
let access_blocked = (mix).0 + (mix).`1` + (mix).a + (mix).`b`

let [a, b, c, d, e] = positional
let [.a, .b, .c, .d, .e] = named

let [
	.0   : num
	.1   : num
	.`a` : num
] = mix

let foo = [... [1, 2, 3], 4, 5]
let [.4 a, .5 b, ... bar] = foo